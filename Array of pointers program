
#include <stdio.h>

int main() {
    int num1 = 10, num2 = 20, num3 = 30;
    int *ptrArr[3]; // Array of pointers to integers

    ptrArr[0] = &num1;
    ptrArr[1] = &num2;
    ptrArr[2] = &num3;

    for (int i = 0; i < 3; i++) {
        printf("Value at ptrArr[%d] = %d\n", i, *ptrArr[i]);
    }

    return 0;
}
